{
  "name": "rolodink-extension-ui",
  "private": true,
  "version": "1.1.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc --noEmit --project tsconfig.app.json",
    "build": "tsc --project tsconfig.app.json && vite build && node build-background.cjs && node copy-assets.cjs",
    "postbuild:firefox": "node firefox-postbuild.cjs",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 20",
    "preview": "vite preview",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.74.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "terser": "^5.44.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}